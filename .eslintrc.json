{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "globals": {
      "document": true,
      "window": true,
      "process": true
    },
    "rules": {
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "react/react-in-jsx-scope": 0
    }
}